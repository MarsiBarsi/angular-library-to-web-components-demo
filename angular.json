{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "angular-library-to-web-components-demo": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "less",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:module": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/angular-library-to-web-components-demo",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "assets": ["src/favicon.ico", "src/assets"],
                        "styles": ["src/styles.less"],
                        "scripts": [
                            {
                                "input": "node_modules/document-register-element/build/document-register-element.js"
                            }
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "angular-library-to-web-components-demo:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "angular-library-to-web-components-demo:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "angular-library-to-web-components-demo:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["tsconfig.app.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        },
        "elements": {
            "projectType": "application",
            "schematics": {},
            "root": "projects/elements",
            "sourceRoot": "projects/elements/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "customWebpackConfig": {
                            "path": "./projects/elements/elements-webpack.config.js"
                        },
                        "outputPath": "projects/elements/dist/tmp",
                        "index": "projects/elements/src/index.html",
                        "main": "projects/elements/src/main.ts",
                        "polyfills": "projects/elements/src/polyfills.ts",
                        "tsConfig": "projects/elements/tsconfig.app.json",
                        "styles": ["projects/elements/src/styles.less"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "elements:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "elements:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "elements:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["projects/elements/tsconfig.app.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        },
        "angular-components-library": {
            "projectType": "library",
            "root": "projects/angular-components-library",
            "sourceRoot": "projects/angular-components-library/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/angular-components-library/tsconfig.lib.json",
                        "project": "projects/angular-components-library/ng-package.json"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["projects/angular-components-library/tsconfig.lib.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        },
        "another-angular-components-library": {
            "projectType": "library",
            "root": "projects/another-angular-components-library",
            "sourceRoot": "projects/another-angular-components-library/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/another-angular-components-library/tsconfig.lib.json",
                        "project": "projects/another-angular-components-library/ng-package.json"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["projects/another-angular-components-library/tsconfig.lib.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        }
    },
    "defaultProject": "angular-library-to-web-components-demo"
}
